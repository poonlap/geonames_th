<odoo>
<function model="ir.config_parameter" name="set_param" eval="('geonames.url', 'https://github.com/poonlap/odoo-th/raw/master/data/th/%s.zip')" />
<function model="res.lang" name="load_lang" eval="('th_TH','th')" />
<record id="base.th" model="res.country">
    <field name="address_format">%(street)s
%(street2)s
%(city)s %(state_name)s %(zip)s
%(country_name)s</field>
</record>

